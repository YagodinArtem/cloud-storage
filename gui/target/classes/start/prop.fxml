<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="593.0"
      prefWidth="801.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controller.Controller">

    <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button mnemonicParsing="false" onAction="#chose" text="Chose file" GridPane.columnIndex="2"
                        GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER"
                        GridPane.vgrow="ALWAYS">
                    <font>
                        <Font size="10.0"/>
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#deleteFileFromClient" text="Delete file"
                        GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                        GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                    <font>
                        <Font size="10.0"/>
                    </font>
                </Button>
                <Button layoutX="92.0" layoutY="13.0" mnemonicParsing="false" onAction="#dirRight" text="→"
                        GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                        GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                    <font>
                        <Font size="10.0"/>
                    </font>
                </Button>
                <Button layoutX="87.0" layoutY="15.0" mnemonicParsing="false" onAction="#dirLeft" text="←"
                        GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER"
                        GridPane.vgrow="ALWAYS">
                    <font>
                        <Font size="10.0"/>
                    </font>
                </Button>
            </children>
        </GridPane>
        <TextArea fx:id="clientCurrentFolder" maxHeight="25.0" onKeyPressed="#enterPath" prefHeight="145.0"
                  prefWidth="233.0"/>
        <ListView fx:id="clientView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
        <TextArea fx:id="clientText" editable="false" maxHeight="50.0" minHeight="50.0" prefHeight="200.0"
                  prefWidth="200.0" VBox.vgrow="ALWAYS"/>
    </VBox>
    <GridPane maxWidth="40.0" prefHeight="400.0" prefWidth="0.0" HBox.hgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <children>
            <Button mnemonicParsing="false" onAction="#send" text="&gt;&gt;" GridPane.halignment="CENTER"
                    GridPane.valignment="CENTER"/>
            <Button mnemonicParsing="false" onAction="#download" text="&lt;&lt;" GridPane.halignment="CENTER"
                    GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
        </children>
    </GridPane>
    <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button mnemonicParsing="false" onAction="#refresh" text="Refresh" GridPane.halignment="CENTER"
                        GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                    <font>
                        <Font size="10.0"/>
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#deleteFileFromServer" text="Delete file"
                        GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                        GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                    <font>
                        <Font size="10.0"/>
                    </font>
                </Button>
                <MenuButton mnemonicParsing="false" text="Settings" GridPane.columnIndex="2"
                            GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Registration" onAction="#registration" fx:id="reg"/>
                        <MenuItem mnemonicParsing="false" text="Exit"/>
                    </items>
                </MenuButton>
            </children>
        </GridPane>
        <TextArea fx:id="serverCurrentFolder" editable="false" maxHeight="25.0" prefHeight="145.0" prefWidth="233.0"/>
        <ListView fx:id="serverView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
        <TextArea fx:id="serverText" editable="false" maxHeight="50.0" minHeight="50.0" prefHeight="200.0"
                  prefWidth="200.0"/>
    </VBox>
</HBox>
